                                                                                        <dx:TreeListDataColumn FieldName="FECHA" Caption="Fecha" VisibleIndex="1" Width="190"></dx:TreeListDataColumn>
                                                                                        <dx:TreeListDataColumn FieldName="AUTOR" Caption="Autor" VisibleIndex="2" Width="120"></dx:TreeListDataColumn>
                                                                                        <dx:TreeListDataColumn FieldName="FECHA_MOD" Caption="Fecha Mod." VisibleIndex="3" Width="190"></dx:TreeListDataColumn>
                                                                                        <dx:TreeListDataColumn FieldName="AUTOR_MOD" Caption="Autor Mod" VisibleIndex="4" Width="120"></dx:TreeListDataColumn>
                                                                                    </Columns>
                                                                                    <Settings ShowColumnHeaders="True" />
                                                                                    <SettingsBehavior AllowEllipsisInText="True" AllowFocusedNode="True" AutoExpandAllNodes="True" ExpandCollapseAction="NodeDblClick" />
                                                                                    <Settings ScrollableHeight="325" SuppressOuterGridLines="true" VerticalScrollBarMode="Visible" ShowRoot="False" HorizontalScrollBarMode="Visible" />
                                                                                    <ClientSideEvents 
                                                                                        CustomDataCallback="refreshbtnDocFavorito"
                                                                                        FocusedNodeChanged="function(s, e) { 
                                                                                            var key = trlDocumentos.GetFocusedNodeKey();
                                                                                            trlDocumentos.PerformCustomDataCallback(key);
                                                                                        }"
                                                                                        NodeDblClick="function(s, e) { 
                                                                                            pcDirectorioORM.Hide();
                                                                                            btnAceptar.DoClick();
                                                                                        }" />
                                                                                </dx:ASPxTreeList>
                                                                            </dx:SplitterContentControl>
                                                                        </ContentCollection>
                                                                    </dx:SplitterPane>
                                                                </Panes>
                                                            </dx:ASPxSplitter>
                                                        </dx:PanelContent>
                                                    </PanelCollection>
                                                </dx:ASPxPanel>
                                                <dx:ASPxPanel ID="BottomPanel" runat="server" CssClass="bottomPanel">
                                                    <PanelCollection>
                                                        <dx:PanelContent runat="server" SupportsDisabledAttribute="True">
                                                            <dx:ASPxCallback ID="ASPxCallbackFavorite" runat="server" ClientInstanceName="CallbackFavorite"  OnCallback="ASPxCallbackFavorite_Callback"></dx:ASPxCallback>
                                                            <dx:ASPxButton ID="btnDocFavorito" ClientInstanceName="btnDocFavorito" runat="server" Text="" Width="20px" AutoPostBack="False" Style="float: left; margin-right: 8px; margin-top: 12px; margin-left: 12px; margin-bottom: 8px; color: gray !important; font-size: 20px;" 
                                                                 CssPostfix="none" RenderMode="Link" CssClass="far fa-star noStyle">
                                                                <ClientSideEvents Click="function(s, e) { var key = trlDocumentos.GetFocusedNodeKey();
                                                                                if(key !== null && key !== '') {
                                                                                    CallbackFavorite.PerformCallback(key);
                                                                                }
                                                                    }" />
                                                            </dx:ASPxButton>
                                                            <dx:ASPxButton ID="btnCancelar" runat="server" Text="Cancelar" Width="80px" AutoPostBack="False" Style="float: right; margin-right: 8px; margin-top: 8px; margin-bottom: 8px;">
                                                                <ClientSideEvents Click="function(s, e) { trlDocumentos.SetFocusedNodeKey(null); pcDirectorioORM.Hide(); }" />
                                                            </dx:ASPxButton>
                                                            <dx:ASPxButton ID="btnAceptar" ClientInstanceName="btnAceptar" runat="server" Text="Aceptar" Width="80px" AutoPostBack="False" Style="float: right; margin-right: 8px; margin-top: 8px; margin-bottom: 8px;" OnClick="btnAceptar_Click">
                                                                <ClientSideEvents Click="function(s, e) {
                                                                                pcDirectorioORM.Hide();
                                                                                ShowLoadingPanel();
                                                                                var key = trlDocumentos.GetFocusedNodeKey();
                                                                                if(key !== null && key !== '') {
                                                                                    e.processOnServer = true;
                                                                                }
                                                                                else {
                                                                                    ShowMensajeError();
                                                                                }
                                                                            }" />
                                                            </dx:ASPxButton>
                                                        </dx:PanelContent>
                                                    </PanelCollection>
                                                </dx:ASPxPanel>
                                            </dx:PanelContent>
                                        </PanelCollection>
                                    </dx:ASPxPanel>
                                </dx:PopupControlContentControl>
                            </ContentCollection>
                            <ContentStyle>
                                <Paddings PaddingBottom="5px" />
                            </ContentStyle>
                        </dx:ASPxPopupControl>

                        <dx:ASPxLoadingPanel ID="LoadingPanel" runat="server" ClientInstanceName="LoadingPanel"
                            Modal="True">
                        </dx:ASPxLoadingPanel>

                    </div>

                </section>
                <!-- /.content -->
            </div>
            <!-- /.content-wrapper -->

