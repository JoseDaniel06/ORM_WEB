    <script type="text/javascript">
        function ShowLoadingPanel() {
            LoadingPanel.Show();
        }

        function HideLoadingPanel() {
            LoadingPanel.Hide();
        }

        function actualizaFavState() {
            CallbackFavorite.PerformCallback(0);
        } 

        function refreshbtnDocFavorito2(isFavorite) {
            var buttonElement = btnDocFavorito.GetMainElement();
            if (isFavorite == '1') {
                buttonElement.setAttribute('style', 'float: left; margin-right: 8px; margin-top: 8px; margin-left: 12px; margin-bottom: 8px; color: #18bc9c !important; font-size: 20px;');
            }
            else {
                buttonElement.setAttribute('style', 'float: left; margin-right: 8px; margin-top: 8px; margin-left: 12px; margin-bottom: 8px; color: gray !important; font-size: 20px;');
            }
        }

        function refreshbtnDocFavorito(s, e) {
            var buttonElement = btnDocFavorito.GetMainElement();
            if (e.result == '1') {
                buttonElement.setAttribute('style', 'float: left; margin-right: 8px; margin-top: 8px; margin-left: 12px; margin-bottom: 8px; color: #18bc9c !important; font-size: 20px;');
            }
            else {
                buttonElement.setAttribute('style', 'float: left; margin-right: 8px; margin-top: 8px; margin-left: 12px; margin-bottom: 8px; color: gray !important; font-size: 20px;');
            }
        }

        function ShowPopup(msg) {
            console.log(msg);
            $('#myModal').modal('show');
        }

        function ConsoleLog(msg) {
            console.log(msg);
        }
    </script>

